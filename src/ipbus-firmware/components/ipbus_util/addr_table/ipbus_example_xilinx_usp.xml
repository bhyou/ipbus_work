<?xml version="1.0" encoding="ISO-8859-1"?>

<node>

  <node id="device_dna"
        module="file://ipbus_device_dna_us_usp.xml"
        address="0x00000100" />

  <!-- NOTE: Remember that the lower nine bits are sent straight to
       the sysmon. So keep this at an appropriate offset from 0x0. -->
  <node id="sysmon"
        module="file://ipbus_sysmon_usp_int_ref.xml"
        address="0x00010000" />

  <node id="icap"
        module="file://ipbus_icap.xml"
        address="0x00020000" />

  <node id="iprog"
        module="file://ipbus_iprog.xml"
        address="0x00030000" />

  <node id="axi4lite_master"
        module="file://ipbus_axi4lite_master.xml"
        address="0x00040000" />

  <node id="axi_readback_reg"
        address="0x00041000"
        fwinfo="endpoint;width=0">
        <node id="word0" address="0x0" />
  </node>

</node>
