defaulte: help

help:
	@echo "==============================================="
	@echo "    make target_name  "
	@echo "  vivado  =>  running vivado project for kc705 gmii demo"
	@echo "  dclean  =>  delete all files generated by vivado"
	@echo "  clean   =>  delete jou and log files "
	@echo "==============================================="

vivado: clean
	vivado -mode tcl -source make_proj.tcl

clean:
	@rm -fr vivado.log vivado.jou *.jou *.log 

dclean: clean
	rm -fr  *.cache *.hw *.ip_user_files *.gen *.runs .Xil 
